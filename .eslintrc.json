{
  "extends": [
    "@vkontakte"
  ],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "guard-for-in": "off",
    "comma-dangle": [
      "error",
      "never"
    ]
  }
}